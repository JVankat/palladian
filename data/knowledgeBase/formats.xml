<?xml version="1.0" encoding="windows-1250"?>
<!-- 
  this file specifies more specific formats for attributes of concepts that can improve
  extraction precision compared to the standard data types
-->
<formats>
  <format concept="Country">
    <description attribute="geographic coordinates">desc</description>
    <description attribute="calling code">+NUMBER</description>
  </format>
  <format concept="Car">
    <description attribute="horsepower">hp</description>
    <description attribute="torque">100</description>
  </format>  
</formats>